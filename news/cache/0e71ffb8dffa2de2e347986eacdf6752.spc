g/dc/elements/1.1/";a:1:{s:7:"creator";a:1:{i:0;a:5:{s:4:"data";s:10:"Rich Lowry";s:7:"attribs";a:0:{}s:8:"xml_base";s:38:"https://www.nationalreview.com/rss.xml";s:17:"xml_base_explicit";b:1;s:8:"xml_lang";s:0:"";}}}}}i:16;a:6:{s:4:"data";s:49:"
    
    
    
       
    
        
        
