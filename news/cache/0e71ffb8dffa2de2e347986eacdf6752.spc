"";a:1:{s:11:"isPermaLink";s:5:"false";}}s:8:"xml_base";s:38:"https://www.nationalreview.com/rss.xml";s:17:"xml_base_explicit";b:1;s:8:"xml_lang";s:0:"";}}}s:32:"http://purl.org/dc/elements/1.1/";a:1:{s:7:"creator";a:1:{i:0;a:5:{s:4:"data";s:9:"NRO Staff";s:7:"attribs";a:0:{}s:8:"xml_base";s:38:"https://www.nationalreview.com/rss.xml";s:17:"xml_base_explicit";b:1;s:8:"xml_lang";s:0:"";}}}}}i:20;a:6:{s:4:"data";s:49:"
    
    
    
       
    
        
        
